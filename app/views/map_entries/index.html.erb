<p id="notice"><%= notice %></p>

<h1>Map Entries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Plz</th>
      <th>Location</th>
      <th>Street</th>
      <th>Phone</th>
      <th>Spec</th>
      <th>Lat</th>
      <th>Lng</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @map_entries.each do |map_entry| %>
      <tr>
        <td><%= map_entry.name %></td>
        <td><%= map_entry.plz %></td>
        <td><%= map_entry.location %></td>
        <td><%= map_entry.street %></td>
        <td><%= map_entry.phone %></td>
        <td><%= map_entry.spec %></td>
        <td><%= map_entry.lat %></td>
        <td><%= map_entry.lng %></td>
        <td><%= link_to 'Show', map_entry %></td>
        <td><%= link_to 'Edit', edit_map_entry_path(map_entry) %></td>
        <td><%= link_to 'Destroy', map_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Map Entry', new_map_entry_path %>
